{
  "kind": "collectionType",
  "collectionName": "expedientes",
  "info": {
    "singularName": "expediente",
    "pluralName": "expedientes",
    "displayName": "Expediente"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Fecha_Creacion": {
      "type": "date",
      "required": true
    },
    "estado": {
      "type": "enumeration",
      "required": true,
      "default": "Activo",
      "enum": [
        "Activo",
        "Inactivo",
        "Archivado",
        "Tratamiento"
      ]
    },
    "paciente": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::paciente.paciente",
      "mappedBy": "expediente"
    },
    "resumen_clinicos": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::resumen-clinico.resumen-clinico",
      "mappedBy": "expediente"
    }
  }
}
