{
  "kind": "collectionType",
  "collectionName": "servicios",
  "info": {
    "singularName": "servicio",
    "pluralName": "servicios",
    "displayName": "Servicio"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Nombre_Servicio": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "Descripcion": {
      "type": "text"
    },
    "Costo_base": {
      "type": "decimal",
      "required": true,
      "min": 0,
      "max": 999999
    },
    "Duracion_Minutos": {
      "type": "integer",
      "required": true
    },
    "Categoria": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "preventivo",
        "restaurativo",
        "ortodoncia",
        "emergencia"
      ]
    }
  }
}
