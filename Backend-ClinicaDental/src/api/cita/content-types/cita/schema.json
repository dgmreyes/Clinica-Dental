{
  "kind": "collectionType",
  "collectionName": "citas",
  "info": {
    "singularName": "cita",
    "pluralName": "citas",
    "displayName": "Cita"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Fecha": {
      "type": "date",
      "required": true
    },
    "Hora": {
      "type": "time",
      "required": true
    },
    "Motivo_Consulta": {
      "type": "text",
      "required": true
    },
    "servicio": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::servicio.servicio"
    },
    "Estado": {
      "type": "enumeration",
      "required": true,
      "default": "Pendiente",
      "enum": [
        "Pendiente",
        "Confirmada",
        "Completada",
        "Cancelada"
      ]
    },
    "dentista": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::dentista.dentista",
      "inversedBy": "cita"
    },
    "paciente": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::paciente.paciente",
      "inversedBy": "cita"
    }
  }
}
